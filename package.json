{
  "name": "aepsdk-rulesengine-typescript",
  "version": "1.0.0",
  "description": "A TypeScript implementation of the Adobe Mobile SDK rules engine.",
  "type": "module",
  "module": "dist/es/index.js",
  "main": "dist/cjs/index.cjs",
  "types": "types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/es/index.js",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types"
  ],
  "scripts": {
    "add-license": "node tools/add-license.cjs",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "format": "prettier --write \"{src,test}/**/*.{js,cjs,ts}\" \"**/*.md\"",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick",
    "test": "NODE_ENV=development jest"
  },
  "author": "Adobe",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run add-license"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/jest": "^29.5.0",
    "babel-jest": "^29.5.0",
    "handlebars": "^4.7.7",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.0.2"
  }
}
